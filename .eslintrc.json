{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "eol-last": ["error", "always"],
        "no-unused-vars": ["warn"],
        "no-console": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off"
    }
}
